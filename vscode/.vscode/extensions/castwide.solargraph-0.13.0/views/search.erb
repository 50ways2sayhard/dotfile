<h1>Search Results for <kbd><%= params['query'] %></kbd></h1>

<% unless @results.empty? %>
    <ul class="doc-list">
    <% @results.each do |result| %>
        <li>
            <% esc = URI.escape("\"solargraph:/document?#{result}\"").gsub('%23', URI.escape('%23')) %>
            <a href="command:solargraph._openDocument?<%= esc %>"><%= result %></a>
        </li>
    <% end %>
    </ul>
<% end %>
