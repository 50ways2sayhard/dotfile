	"content_for":{
		"prefix": "content_for",
		"body": [
			"<% content_for :$1 do %>",
			"$2",
			"<% end %>"
		],
		"description": "rails content_for"
	},
	"content_for?":{
		"prefix": "content_for?",
		"body": [
			" <%= content_for?(:$1) ? yield(:$1) : $2%>"
		],
		"description": "rails content_for?"
	},
	"begin":{
		"prefix": "=begin",
		"body": [
			"<%\n=begin %>",

			"$1",
			"<%\n=end %>"
		],
		"description": "rails comment"
	},
	"form_for":{
		"prefix": "form_for",
		"body": [
			"<%= form_for( ${variable} , remote: ${true}) do |f| %> %>",
			"$2",
			"<% end %>"
		],
		"description": "rails form_for"
	},
	"form_tag":{
		"prefix": "form_tag",
		"body": [
			"<%= form_tag( '${path}' , remote: ${true}) do |f| %> %>",
			"$2",
			"<% end %>"
		],
		"description": "rails form_tag"
	}
